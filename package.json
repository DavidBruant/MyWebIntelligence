{
  "name": "MyWebIntelligence",
  "version": "0.0.0",
  "description": "Understanding the web around you",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.3 <0.12"
  },
  "scripts": {
    "postinstall": "cp git-hooks/pre-commit .git/hooks/pre-commit; chmod a+x .git/hooks/pre-commit",
    "test": "mocha test/automated/database/* test/automated/regexp-not-only-spaces.js",
    "start": "node server/index.js",
    "build": "docker build --tag=\"mywi/base\" .",
    "start-db-container": "docker rm mywipostgres; docker run --name mywipostgres -p 5555:5432 -e POSTGRES_PASSWORD=password -d postgres:9.4",
    "start-test-db-container": "docker run -p 6666:5432 -e POSTGRES_PASSWORD=password -d postgres:9.4",
    "start-prod": "docker run -d -p=3333:3333 -e \"NODE_ENV=production\" --link mywipostgres:mywipostgres mywi/base node server/index.js",
    "start-dev": "docker run -p=3333:3333 -v $PWD/node_modules:/usr/mywi/node_modules -e \"NODE_ENV=dev-docker\" -m=\"800m\" --link mywipostgres:mywipostgres mywi/base node server/index.js",
    "bundle": "jsx node_modules/react-tabs-component/ client/components/external/ Tabs && browserify client/index.js -o client/browserify-bundle.js -d",
    "watch": "watchify client/index.js -o client/browserify-bundle.js -d -v",
    "lint": "eslint ./",
    "build-test-crawl-docker-image": "docker build --tag=\"mywi/test-crawl\" --file=test/automated/crawl/Dockerfile .",
    "test-crawl": "docker run --add-host a.web:127.0.0.1 mywi/test-crawl /usr/bin/supervisord -c /usr/mywi/test/automated/crawl/supervisord.conf"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MyWebIntelligence/MyWebIntelligence.git"
  },
  "keywords": [
    "web",
    "crawling",
    "SNA",
    "Social",
    "Networks",
    "text",
    "analysis"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MyWebIntelligence/MyWebIntelligence/issues"
  },
  "dependencies": {
    "body-parser": "^1.10.1",
    "browserify": "~8.0.3",
    "compression": "~1.3.0",
    "es-env-polyfills": "^1.0.0",
    "es6-promisify": "^1.1.1",
    "es6-set": "~0.1.1",
    "express": "~4.10.7",
    "express-session": "~1.10.1",
    "fast-csv": "^0.6.0",
    "jsdom": "3.x",
    "moment": "^2.10.2",
    "multer": "^0.1.7",
    "page": "^1.6.3",
    "passport": "~0.2.1",
    "passport-google-oauth": "~0.1.5",
    "pg": "^4.3.0",
    "react": "~0.12.2",
    "react-tabs-component": "^1.1.3",
    "react-tools": "^0.13.2",
    "request": "^2.53.0",
    "sql": "^0.52.0",
    "stringmap": "^0.2.2"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-as-promised": "^4.1.1",
    "eslint": "^0.15.1",
    "mocha": "^2.1.0",
    "mock-require": "^1.0.5",
    "tmp": "0.0.24",
    "watchify": "~2.2.1"
  }
}
